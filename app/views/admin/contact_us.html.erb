<!--  page-wrapper -->
<div id="page-wrapper" style="padding-top:6%" >

  <!-- row -->

  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-body">
        <h3>List of all Users</h3>
        <div class="table-responsive">

          <table class="table table-bordered table-striped">
            <tr>
              <th>No</th>
              <th>Name  </th>
              <th>Email</th>
              <th>Description</th>
              <th>Date</th>
              <th>Action</th>
            </tr>
            <% i = 1 %>
            <% @contactus.each do |contactus| %>
              <tr>
                <td><%= i %></td>
                <td><%= contactus.name%> </td>
                <td><%= contactus.email if contactus.email%> </td>
                <td><%= contactus.subject if contactus.subject%> </td>
                <td><%=contactus.created_at if contactus.created_at%> </td> 
                <td>
                  <% if  %>
                    <%= link_to "#{contactus.active ? 'Open' : 'Close' }", admin_active_path(contactus.id) if contactus.active.present? %>
                  <%else%>
                    Nil
                  <%end%> 
                </td>
              </tr>
              <% i +=1 %>
            <% end if @contactus%>
          </table>
        </div>          
      </div>
    </div>
  </div>
</div>
